# Chat formatting for faction action responses.
faction_action_format:
    type: format
    format: <&lb><reset><&color[#1569EA]>Faction Action<reset><&rb><reset><&co> <[text]>

# Chat formatting for faction action errors.
faction_action_error_format:
    type: format
    format: <&lb><reset><&color[#1569EA]>Faction Action<reset><&co> <red><bold>Error!<reset><&rb><&co> <[text]>
