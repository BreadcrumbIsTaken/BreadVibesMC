# Chat formatting for faction action responses.
faction_action_format:
    type: format
    format: <bold>[<reset><&color[#1569EA]>Faction Action<reset><bold>]<reset><&co> <[text]>

# Chat formatting for faction action errors.
faction_action_error_format:
    type: format
    format: <bold>[<reset><&color[#1569EA]>Faction Action<reset><&co> <red><bold>Error!<reset>]<&co> <[text]>
